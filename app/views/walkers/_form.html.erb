<%= form_for @walker do |f| %>
    <% if flash[:walker_errors] %>
        <% flash[:walker_errors].each do |error| %>
            <%= error %>
        <% end %>
    <% end %>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :image_url %>
    <%= f.text_field :image_url %>
    
    <%= f.submit %>
<% end %>